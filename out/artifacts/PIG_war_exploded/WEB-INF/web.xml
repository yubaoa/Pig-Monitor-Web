<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>controller.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Piginformation</servlet-name>
        <servlet-class>controller.PiginfServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Piginformation</servlet-name>
        <url-pattern>/Piginf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>houseinf</servlet-name>
        <servlet-class>controller.PighouseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>houseinf</servlet-name>
        <url-pattern>/House</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Piginflist</servlet-name>
        <servlet-class>controller.PigrecordlistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Piginflist</servlet-name>
        <url-pattern>/Pigrecord</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Pighousedetail</servlet-name>
        <servlet-class>controller.HousedetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Pighousedetail</servlet-name>
        <url-pattern>/Housedetail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Piglist</servlet-name>
        <servlet-class>controller.PiglistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Piglist</servlet-name>
        <url-pattern>/Piglist</url-pattern>
    </servlet-mapping>
</web-app>